{
  "name": "vue-nav-schema",
  "version": "1.0.0",
  "description": "> Build reactive navigation schemas from Vue Router configuration",
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "vue",
    "vue-router",
    "navigation",
    "menu"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danillistov/vue-nav-schema.git"
  },
  "author": "Danil Listov<https://github.com/danillistov>",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/danillistov/vue-nav-schema/issues"
  },
  "homepage": "https://github.com/danillistov/vue-nav-schema#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/node": "^25.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitest/ui": "^4.0.15",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "happy-dom": "^20.0.11",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vitest": "^4.0.15",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "peerDependencies": {
    "vue": "^3.3.0",
    "vue-router": "^4.0.0"
  },
  "dependencies": {
    "es-toolkit": "^1.42.0"
  }
}
